<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <PropertyPageSchema Condition="'$(Platform)' == 'Win32'" Include="$(MSBuildThisFileDirectory)inteldaal.devel.win-x86.xml"/>
  </ItemGroup>

  <!-- set path to include and libraries -->
  <!-- dynamic libraries -->
  <ItemDefinitionGroup Condition="'$(Platform)' == 'Win32' And '$(linkage-DAAL-dynamic-win-x86)' == 'dynamic'">
    <ClCompile>
      <AdditionalIncludeDirectories>$(MSBuildThisFileDirectory)..\..\lib\native\include;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>$(MSBuildThisFileDirectory)..\..\lib\native\win-x86;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalDependencies>daal_core_dll.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
</Project>
